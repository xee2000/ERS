<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper	namespace="kr.ac.ers.repository.ReportFileMapper">
	<update id="insertFile" parameterType="ReportFileVO">
	insert into
	reportfile(sfNo,attacher,rNo,uploadpath,filename,filetype,regDate)
	values(#{sfNo},#{attacher},#{rNo},#{uploadpath},#{filename},#{regDate)
	</update>
</mapper>